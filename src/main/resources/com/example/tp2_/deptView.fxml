<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane fx:id="DeptPane" prefHeight="237.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.tp2_.Controller.DeptViewController">
   <children>
      <VBox prefHeight="239.0" prefWidth="632.0">
         <children>
            <HBox alignment="CENTER">
               <children>
                  <Button fx:id="AjouterButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="100.0" style="-fx-background-color: #e9c46a;" text="Add">
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <HBox.margin>
                        <Insets right="40.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="SupprimerButton" mnemonicParsing="false" prefWidth="100.0" style="-fx-background-color: #e9c46a;" text="Supp">
                     <HBox.margin>
                        <Insets right="40.0" />
                     </HBox.margin>
                  </Button>
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <padding>
                  <Insets bottom="10.0" />
               </padding>
            </HBox>
            <TableView fx:id="TableDept" prefHeight="208.0" prefWidth="780.0" style="-fx-border-color: none;" stylesheets="@styles/tabelview.css" VBox.vgrow="ALWAYS">
               <columns>
                  <TableColumn fx:id="idDept" minWidth="-1.0" prefWidth="136.0" text="id" />
                  <TableColumn fx:id="nomDept" minWidth="-1.0" prefWidth="64.0" text="nom" />
               </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </TableView>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</StackPane>
